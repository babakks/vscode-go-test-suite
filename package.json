{
    "name": "vscode-go-check-test-suite",
    "displayName": "Go Check Test Suite Explorer",
    "description": "Explore Check test suites for Go",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.76.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "extensionDependencies": [
        "golang.go"
    ],
    "contributes": {
        "commands": [
            {
                "command": "vscode-go-check-test-suite.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "test-local": "npm run compile && node ./node_modules/mocha/bin/mocha.js ./out/test/local/*.test.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.76.0",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "@vscode/test-electron": "^2.3.0",
        "eslint": "^8.36.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "vscode-test-adapter-api": "^1.9.0",
        "vscode-test-adapter-util": "^0.7.1"
    }
}