{
    "name": "vscode-go-test-suite",
    "displayName": "Go Test Suite Support",
    "description": "VS Code extension to click and run Go test functions written in third-party library formats",
    "version": "0.0.15",
    "publisher": "babakks",
    "repository": {
        "type": "git",
        "url": "https://github.com/babakks/vscode-go-test-suite"
    },
    "engines": {
        "vscode": "^1.72.0"
    },
    "categories": [
        "Testing"
    ],
    "keywords": [
        "go",
        "golang",
        "test",
        "suite",
        "run",
        "debug",
        "gocheck",
        "check",
        "go-check",
        "quicktest",
        "qt",
        "qtsuite"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "icon": "media/go.png",
    "contributes": {
        "commands": [
            {
                "command": "vscode-go-test-suite._moveCursorAndExecuteTest",
                "title": "_moveCursorAndExecuteTest",
                "enablement": "false"
            },
            {
                "command": "vscode-go-test-suite._showLaunchConfiguration",
                "title": "_showLaunchConfiguration",
                "enablement": "false"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "test-local": "npm run compile && node ./node_modules/mocha/bin/mocha.js ./out/test/local/*.test.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.72.0",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "@vscode/test-electron": "^2.3.0",
        "eslint": "^8.36.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@vscode/extension-telemetry": "^0.7.7"
    }
}
